<h1>TinyApp</h1>
<% if (username) { %>
  <form action="/logout" method="POST">Logged In As: <%= username %>
    <br>
    <button class="btn btn-primary">Logout</button>
  </form>
<% } else { %>
  <form action="/login" method="POST">
    <input class="form-control" name="username" style="width: 300px">
    <button class="btn btn-primary">Login</button>
  </form>
<% }%>
